{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    {
      "type": "panel",
      "id": "timeseries",
      "name": "Time series",
      "version": ""
    },
    {
      "type": "panel",
      "id": "stat",
      "name": "Stat",
      "version": ""
    },
    {
      "type": "panel",
      "id": "gauge",
      "name": "Gauge",
      "version": ""
    },
    {
      "type": "datasource",
      "id": "prometheus",
      "name": "Prometheus",
      "version": ""
    }
  ],
  "title": "Torarr Overview",
  "uid": "torarr-overview",
  "schemaVersion": 39,
  "version": 1,
  "editable": true,
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "datasource",
        "type": "datasource",
        "query": "prometheus",
        "label": "Data Source",
        "current": {
          "selected": true,
          "text": "Prometheus",
          "value": "Prometheus"
        }
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Tor Ready",
      "datasource": "${DS_PROMETHEUS}",
      "targets": [
        {
          "expr": "torarr_tor_ready",
          "legendFormat": "Ready"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "gridPos": {
        "h": 4,
        "w": 4,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "type": "gauge",
      "title": "Bootstrap Progress",
      "datasource": "${DS_PROMETHEUS}",
      "targets": [
        {
          "expr": "torarr_tor_bootstrap_percent",
          "legendFormat": "Bootstrap"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "min": 0,
        "max": 100,
        "thresholds": {
          "mode": "absolute",
          "steps": [
            {
              "color": "red",
              "value": null
            },
            {
              "color": "yellow",
              "value": 50
            },
            {
              "color": "green",
              "value": 95
            }
          ]
        }
      },
      "gridPos": {
        "h": 4,
        "w": 4,
        "x": 4,
        "y": 0
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Circuits Established",
      "datasource": "${DS_PROMETHEUS}",
      "targets": [
        {
          "expr": "torarr_tor_circuit_established",
          "legendFormat": "Established"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "gridPos": {
        "h": 4,
        "w": 4,
        "x": 8,
        "y": 0
      }
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "Tor Traffic (Bytes)",
      "datasource": "${DS_PROMETHEUS}",
      "targets": [
        {
          "expr": "torarr_tor_bytes_read",
          "legendFormat": "Bytes Read"
        },
        {
          "expr": "torarr_tor_bytes_written",
          "legendFormat": "Bytes Written"
        }
      ],
      "options": {
        "legend": {
          "showLegend": true
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 4
      }
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "HTTP Requests (5m rate)",
      "datasource": "${DS_PROMETHEUS}",
      "targets": [
        {
          "expr": "sum by (path, method) (rate(torarr_http_requests_total[5m]))",
          "legendFormat": "{{path}} {{method}}"
        }
      ],
      "options": {
        "legend": {
          "showLegend": true
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 12
      }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "External Check Outcomes (5m rate)",
      "datasource": "${DS_PROMETHEUS}",
      "targets": [
        {
          "expr": "sum by (endpoint, success, is_tor) (rate(torarr_external_check_total[5m]))",
          "legendFormat": "{{endpoint}} success={{success}} tor={{is_tor}}"
        }
      ],
      "options": {
        "legend": {
          "showLegend": true
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 20
      }
    }
  ]
}
